@model IWantMyMummy.Models.BurialSquare

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container">

    <h1>Additional Square Details</h1>
    <br /><br />

    <div>
        <hr />
        <dl class="row">
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.BurialLocationNs)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.BurialLocationNs)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.BurialLocationEw)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.BurialLocationEw)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.LowPairNs)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.LowPairNs)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.HighPairNs)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.HighPairNs)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.LowPairEw)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.LowPairEw)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.HighPairEw)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.HighPairEw)
            </dd>
        </dl>
    </div>
    <div>
        <a asp-action="Edit" asp-route-id="@Model.BurialSquareId">Edit</a> |
        <a asp-action="Index">Back to Squares</a>
    </div>
 </div>