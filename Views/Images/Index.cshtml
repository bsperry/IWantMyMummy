@model IEnumerable<IWantMyMummy.Models.Image>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Image1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ImageDescription)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Burial)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BurialS)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BurialSquare)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Cranial)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Image1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ImageDescription)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Burial.BurialWrapping)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BurialS.BurialSubplot)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BurialSquare.BurialSquareId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Cranial.CranialId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ImageId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ImageId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ImageId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
