@model BurialInformationViewModel
@*@model IWantMyMummy.Models.Burial*@
@{
    ViewData["Title"] = "Edit";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container">
    <h1>Edit Burial</h1>
    <br /><br />
    <div class="row">
        <div class="col-md-4 col-centered">
            <form asp-action="Edit">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <input type="hidden" asp-for="@Model.Burial.BurialId" />
                @if (Model.Burial.BurialSubplot == null)
                {
                    <p>IMPORTANT: You can either change the Square or the Subplot, not both</p>
                    <div class="form-group">
                        <label asp-for="@Model.Burial.BurialSquareId" class="control-label"></label>
                        <select asp-for="@Model.Burial.BurialSquareId" class="form-control" asp-items="ViewBag.BurialSquareId">Burial Square Id</select>
                        <span asp-validation-for="@Model.Burial.BurialSquareId" class="text-danger"></span>
                    </div>

                    <div class="form-group">
                        <label asp-for="@Model.Burial.BurialSubplot" class="control-label"></label>
                        <select asp-for="@Model.Burial.BurialSubplot" class="form-control">
                            <option value="">No Subplot</option>
                            @foreach (var quads in Model.BurialQuadrantsList)
                            {
                                @if (quads.BurialSubplot != Model.Burial.BurialSubplot)
                                {
                                    <option>@quads.BurialSubplot</option>
                                }
                            }
                        </select>
                        <span asp-validation-for="BurialSubplot" class="text-danger"></span>
                    </div>
                }
                @if (Model.Burial.BurialSubplot != null)
                {
                    <div class="form-group">
                        <label asp-for="@Model.Burial.BurialSquareId" class="control-label"></label>
                        <p>@Model.Burial.BurialSquareId</p>
                        <input type="hidden" asp-for="@Model.Burial.BurialSquareId" class="form-control" value="@Model.Burial.BurialSquareId">
                        <span asp-validation-for="@Model.Burial.BurialSquareId" class="text-danger"></span>
                    </div>

                    <div class="form-group">
                        <label asp-for="@Model.Burial.BurialSubplot" class="control-label"></label>
                        <select asp-for="@Model.Burial.BurialSubplot" class="form-control">
                            <option>@Model.Burial.BurialSubplot</option>
                            @foreach (var quads in Model.BurialQuadrantsList)
                            {
                                @if (quads.BurialSubplot != Model.Burial.BurialSubplot)
                                {
                                    <option>@quads.BurialSubplot</option>
                                }
                            }
                            <option value="">No Subplot</option>
                        </select>
                        <span asp-validation-for="BurialSubplot" class="text-danger"></span>
                    </div>
                }

                <div class="form-group">
                    <label asp-for="@Model.Burial.BurialNumber" class="control-label">Burial Number</label>
